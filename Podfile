# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Nimble Survey App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	pod 'SVProgressHUD'
    pod 'Alamofire', '~> 5.0.0-beta.5'
    pod 'SwiftyJSON'
    pod 'SDWebImage'

  # Pods for Nimble Survey App

  target 'Nimble Survey AppTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'SVProgressHUD'
    pod 'Alamofire', '~> 5.0.0-beta.5'
    pod 'SwiftyJSON'
    pod 'SDWebImage'
  end

  target 'Nimble Survey AppUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'SVProgressHUD'
    pod 'Alamofire', '~> 5.0.0-beta.5'
    pod 'SwiftyJSON'
    pod 'SDWebImage'
  end

end
